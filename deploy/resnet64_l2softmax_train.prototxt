name: "face_res20net"
layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label000"
  include {
    phase: TRAIN
  }
  transform_param {
    crop_size: 128
    scale: 0.0078125
    mean_value: 127.5
    mirror: true
  }
  data_param {
    source: "./data/casia-webface/p_noise-flip-outlier-1_1-40/converter_p_casia-webface_noise-flip-outlier-1_1-40/train_data_db/"
    backend: LMDB
    batch_size: 64
  }
}
layer {
  name: "data"
