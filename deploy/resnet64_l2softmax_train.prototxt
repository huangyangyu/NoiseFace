####################### innerproduct ###################
layer {
  name: "fc5_norm"
  type: "NormL2"
  bottom: "fc5"
  top: "fc5_norm"
}
layer {
  name: "fc6"
  type: "InnerProduct"
  bottom: "fc5_norm"
  top: "fc6"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 10575
    normalize: true
    weight_filler {
      type: "xavier"
    }
    bias_term: false
  }
}
layer {
  name: "fix_cos"
  type: "NoiseTolerantFR"
  bottom: "fc6"
  bottom: "fc6"
  bottom: "label000"
  top: "fix_cos"
  noise_tolerant_fr_param {
    shield_forward: false
    start_iter: 1
    bins: 200
    slide_batch_num: 1000
    
    value_low: -1.0
    value_high: 1.0
    
    #debug: true
    #debug_prefix: "p_casia-webface_noise-flip-outlier-1_1-40_Nsoftmax_AD"
    debug: false
  }
}
layer {
  name: "fc6_margin_scale"
  type: "Scale"
  bottom: "fix_cos"
  top: "fc6_margin_scale"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  scale_param {
    filler {
      type: "constant"
      value: 32.0
    }
    bias_term: false
  }
}
######################### softmax #######################
layer {
  name: "loss/label000"
  type: "SoftmaxWithLoss"
  bottom: "fc6_margin_scale"
  bottom: "label000"
  top: "loss/label000"
  loss_weight: 1.0
  loss_param {
    ignore_label: -1
  }
}
layer {
  name: "accuracy/label000"
  type: "Accuracy"
  bottom: "fc6_margin_scale"
  bottom: "label000"
  top: "accuracy/label000"
  accuracy_param {
    top_k: 1
    ignore_label: -1
  }
}
